<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.utdallas.ui_quiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cole"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuizQuestionActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_quiz_question"
            android:theme="@style/FullscreenTheme" />
        <activity android:name=".NewHighScoreActivity" />
        <activity android:name=".HighScoresActivity"></activity>
    </application>

</manifest>